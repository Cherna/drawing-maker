{
    "sketch": "pipeline",
    "outputBaseName": "pipeline_demo",
    "canvas": {
        "width": 200,
        "height": 300,
        "margin": [
            20,
            20
        ]
    },
    "gcode": {
        "feedRate": 2000,
        "zUp": 5,
        "zDown": 0
    },
    "params": {
        "steps": [
            {
                "tool": "stripes",
                "params": {
                    "lines": 120
                }
            },
            {
                "tool": "resample",
                "params": {
                    "res": 0.5
                }
            },
            {
                "tool": "noise",
                "params": {
                    "scale": 0.05,
                    "magnitude": 5
                }
            },
            {
                "tool": "clone",
                "params": {
                    "steps": [
                        {
                            "tool": "rotate",
                            "params": {
                                "rotation": 5
                            }
                        }
                    ]
                }
            }
        ]
    }
}