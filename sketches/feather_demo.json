{
    "sketch": "pipeline",
    "outputBaseName": "feather_demo",
    "canvas": {
        "width": 300,
        "height": 400,
        "margin": [
            100,
            80
        ]
    },
    "gcode": {
        "feedRate": 2000,
        "zUp": 5,
        "zDown": 0
    },
    "params": {
        "steps": [
            {
                "tool": "stripes",
                "params": {
                    "lines": 150
                }
            },
            {
                "tool": "resample",
                "params": {
                    "res": 0.5
                }
            },
            {
                "tool": "noise",
                "params": {
                    "scale": 0.05,
                    "magnitude": 5
                }
            },
            {
                "tool": "trim",
                "params": {},
                "mask": {
                    "type": "radial",
                    "params": {
                        "center": [
                            0.5,
                            0.5
                        ],
                        "radius": 0.4
                    },
                    "invert": true
                }
            }
        ]
    }
}