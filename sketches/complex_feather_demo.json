{
    "sketch": "pipeline",
    "outputBaseName": "complex_feather_demo",
    "canvas": {
        "width": 300,
        "height": 400,
        "margin": [
            50,
            50
        ]
    },
    "gcode": {
        "feedRate": 2000,
        "zUp": 5,
        "zDown": 0
    },
    "params": {
        "steps": [
            {
                "tool": "stripes",
                "params": {
                    "lines": 200
                }
            },
            {
                "tool": "resample",
                "params": {
                    "res": 0.5
                }
            },
            {
                "tool": "noise",
                "params": {
                    "scale": 0.05,
                    "magnitude": 5
                }
            },
            {
                "tool": "trim",
                "params": {},
                "mask": {
                    "type": "border",
                    "params": {
                        "top": 0.15,
                        "right": 0.25,
                        "bottom": 0.2,
                        "left": 0.3,
                        "centerRadius": 0.25
                    }
                }
            }
        ]
    }
}
