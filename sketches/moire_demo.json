{
    "sketch": "pipeline",
    "outputBaseName": "moire_demo",
    "canvas": {
        "width": 200,
        "height": 200,
        "margin": 20
    },
    "gcode": {
        "feedRate": 2000,
        "zUp": 5,
        "zDown": 0
    },
    "params": {
        "seed": 42,
        "steps": [
            {
                "tool": "stripes",
                "params": {
                    "lines": 80
                }
            },
            {
                "tool": "resample",
                "params": {
                    "res": 0.5
                }
            },
            {
                "tool": "noise",
                "params": {
                    "scale": 0.05,
                    "magnitude": 3,
                    "seed": 42
                }
            },
            {
                "tool": "layer",
                "params": {
                    "id": "rotated_layer",
                    "steps": [
                        {
                            "tool": "rotate",
                            "params": {
                                "rotation": 2.5
                            }
                        }
                    ]
                }
            }
        ]
    }
}
